{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"on": {
			"type": "string"
		},
		"fetchType": {
			"type": "string",
			"enum": [
				"IMMEDIATE",
				"LAZY"
			]
		},
		"where": {
			"type": "string"
		},
		"index": {
			"type": "string"
		},
		"rightIndex": {
			"type": "string"
		},
		"hashSide": {
			"type": "string",
			"enum": [
				"NONE",
				"PROBE",
				"BUILD"
			]
		},
		"keys": {
			"type": "array",
			"items": {
				"type": "string"
			}
		}
	},
	"required": [
		"on"
	]
}
