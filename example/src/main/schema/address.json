{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"title": "Address",
	"type": "object",
	"x-cb-document": true,
	"properties": {
		"id": {
			"title": "Document id",
			"type": "string",
			"x-cb-id": {
				"generated": {
					"delimiter": "::",
					"strategy": "USE_ATTRIBUTES"
				}
			}
		},
		"cas": {
			"title": "Document CAS",
			"type": "integer",
			"format": "int64",
			"x-cb-cas": true
		},
		"idPrefix": {
			"title": "Id prefix",
			"type": "string",
			"default": "address",
			"x-cb-idPrefix": true
		},
		"userId": {
			"title": "Address user id",
			"type": "string",
			"default": "address",
			"x-cb-field": {
				"idAttribute": true
			}
		},
		"street": {
			"title": "Address street",
			"type": "string"
		},
		"zipCode": {
			"title": "Address zip code",
			"type": "string"
		},
		"city": {
			"title": "Address city",
			"type": "string"
		}
	}
}
