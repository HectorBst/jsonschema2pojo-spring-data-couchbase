{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"title": "Pet",
	"type": "object",
	"x-cb-document": true,
	"properties": {
		"id": {
			"title": "Purchase id",
			"type": "string",
			"x-cb-id": {
				"generated": {
					"delimiter": "::",
					"strategy": "UNIQUE"
				}
			}
		},
		"cas": {
			"title": "Document CAS",
			"type": "integer",
			"format": "int64",
			"x-cb-cas": true
		},
		"idPrefix": {
			"title": "Id prefix",
			"type": "string",
			"default": "pet",
			"x-cb-idPrefix": true
		},
		"userId": {
			"title": "Owner user id",
			"type": "string",
			"x-cb-field": {
				"index": true
			}
		},
		"name": {
			"title": "Pet name",
			"type": "string"
		},
		"description": {
			"title": "Pet description",
			"type": "string"
		},
		"type": {
			"title": "Pet type",
			"type": "string",
			"enum": [
				"CAT",
				"DOG",
				"FERRET",
				"GUINEA_PIG",
				"HAMSTER",
				"RABBIT"
			]
		}
	}
}
