{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"title": "Sample simple full entity",
	"type": "object",
	"properties": {
		"id": {
			"title": "Entity id",
			"type": "string",
			"format": "uuid",
			"x-cb-id": true
		},
		"cas": {
			"title": "Couchbase CAS",
			"type": "integer",
			"format": "int64",
			"x-cb-cas": true
		},
		"oneString": {
			"title": "A sample string",
			"type": "string"
		},
		"oneInteger": {
			"title": "A sample integer",
			"type": "integer",
			"format": "int32"
		},
		"oneLong": {
			"title": "A sample long",
			"type": "integer",
			"format": "int64"
		},
		"oneFloat": {
			"title": "A sample float",
			"type": "number",
			"format": "float32"
		},
		"oneDouble": {
			"title": "A sample double",
			"type": "number",
			"format": "float64"
		},
		"oneReal": {
			"title": "A sample big decimal",
			"type": "number",
			"format": "real"
		},
		"oneSubObject": {
			"title": "A sample sub object",
			"type": "object",
			"properties": {
				"anOtherString": {
					"title": "An other sample string",
					"type": "string"
				}
			}
		}
	}
}
